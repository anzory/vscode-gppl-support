{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Define GPPL",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#comments"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.digits.gpp",
          "match": "\\b(([0-9]+\\.?[0-9]*)|(\\.[0-9]+))\\b",
          "comment": "Define Digits"
        },
        {
          "name": "keyword.control.logical.constants.gpp",
          "match": "\\b(([tT](rue)|(RUE))|([fF](alse)|(ALSE)))\\b",
          "comment": "Define Logical Constants"
        },
        {
          "name": "keyword.control.variable.modifier.gpp",
          "match": "\\b(global|local|string|logical|integer|numeric)\\b",
          "comment": "Define GPPL Modifier"
        },
        {
          "name": "keyword.control.logical.operators.gpp",
          "match": "\\b([iI]f|[eE]lse|[eE]lse[iI]f|[eE]nd[iI]f|[tT]hen|[aA]nd|[oO]r|[nN]ot|[eE]q|[nN]e|[lL]e|[lL]t|[gG]e|[gG]t|==|<>|<=|<|>=|>)\\b",
          "comment": "Define GPPL Logical Operators"
        },
        {
          "name": "keyword.control.loop.statement.gpp",
          "match": "\\b([wW]hile|[eE]ndw)\\b",
          "comment": "Define GPPL Loop Execution Statement"
        },
        {
          "name": "keyword.control.stop.statement.gpp",
          "match": "\\b(abort|break|return|exit)\\b",
          "comment": "Define GPPL Stop Execution Statements"
        },
        {
          "name": "keyword.control.logical.flags.gpp",
          "match": "\\b(active|change|even|odd)\\b",
          "comment": "Define GPPL Logical Flags"
        },
        {
          "name": "keyword.control.string.function.gpp",
          "match": "\\b(date|dup|instr|left|lower|replace|rev|right|space|strlen|substr|time|tonum|tostr|trim|upper)\\b",
          "comment": "Define GPPL String Functions"
        },
        {
          "name": "keyword.control.dialog.function.gpp",
          "match": "\\b(print|input)\\b",
          "comment": "Define GPPL Dialog Functions"
        },
        {
          "name": "keyword.control.numeric.function.gpp",
          "match": "\\b(abs|acos|adims|alen|ang|asin|asize|atan|atan2|cos|dist|exp|frac|int|log|log10|mod|norm360|pow|round|round2val|sign|sin|sqrt|square|tan|todeg|torad)\\b",
          "comment": "Define GPPL Numeric Functions"
        },
